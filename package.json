{
  "name": "@tuckn/WshNet",
  "description": "This module provides some useful  WSH (Windows Script Host) functions that handle network on Windows.",
  "version": "1.0.0",
  "author": "Tuckn <tuckn333+github@gmail.com>",
  "private": false,
  "license": "MIT",
  "homepage": "https://docs.tuckn.net/WshNet",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuckn/WshNet.git"
  },
  "bugs": {
    "url": "https://github.com/tuckn/WshNet/issues"
  },
  "keywords": [
    "Windows",
    "WSH",
    "WSF",
    "JScript",
    "VBScript",
    "module",
    "network"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/bundle.js",
  "bin": {},
  "scripts": {
    "install:WshJest": "git submodule add https://github.com/tuckn/WshJest.git ./WshModules/WshJest",
    "install:WshChildProcess": "git submodule add https://github.com/tuckn/WshChildProcess.git ./WshModules/WshChildProcess",
    "installModules": "run-s install:*",
    "updateModules": "git submodule update --remote",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "fix": "run-p fix:*",
    "lint:eslint": "eslint --ext js,ts .",
    "lint:prettier": "prettier -l \"**/*.{json,md,yml,yaml}\"",
    "lint:type": "tsc --noEmit",
    "lint": "run-p lint:*",
    "test:name:respondsHost": ".\\scripts\\Test Net -t respondsHost$",
    "test:name:showIpConfigAll": ".\\scripts\\Test Net -t showIpConfigAll$",
    "test:name:getAdaptersPropsSWbemObjs": ".\\scripts\\Test Net -t getAdaptersPropsSWbemObjs$",
    "test:name:getAdaptersPropsObjs": ".\\scripts\\Test Net -t getAdaptersPropsObjs$",
    "test:name:enablesDHCP": ".\\scripts\\Test Net -t enablesDHCP$",
    "test:name:getAdaptersConfsSWbemObjs": ".\\scripts\\Test Net -t getAdaptersConfsSWbemObjs$",
    "test:name:getAdaptersConfsObjs": ".\\scripts\\Test Net -t getAdaptersConfsObjs$",
    "test:name:getIpSetInAdapters": ".\\scripts\\Test Net -t getIpSetInAdapters$",
    "test:name:getDefaultGateways": ".\\scripts\\Test Net -t getDefaultGateways$",
    "test:name:getDnsIPsSetInAdapters": ".\\scripts\\Test Net -t getDnsIPsSetInAdapters$",
    "test:name:exportWinFirewallSettings": ".\\scripts\\Test Net -t exportWinFirewallSettings$",
    "test:name:setDnsServers": ".\\scripts\\Test Net -t setDnsServers$",
    "test:name:setDnsServersWithWMI": ".\\scripts\\Test Net -t setDnsServersWithWMI$",
    "test:name:setIpAddress": ".\\scripts\\Test Net -t setIpAddress$",
    "test:name:importWinFirewallSettings": ".\\scripts\\Test Net -t importWinFirewallSettings$",
    "test:Net": ".\\scripts\\Test Net",
    "test:name:shareDirectory_READ": ".\\scripts\\Test SMB -t shareDirectory_READ$",
    "test:name:shareDirectory_CHANGE": ".\\scripts\\Test SMB -t shareDirectory_CHANGE$",
    "test:name:showLocalShares": ".\\scripts\\Test SMB -t showLocalShares$",
    "test:name:getLocalSharesSWbemObjs": ".\\scripts\\Test SMB -t getLocalSharesSWbemObjs$",
    "test:name:getLocalSharesObjs": ".\\scripts\\Test SMB -t getLocalSharesObjs$",
    "test:name:existsShareName": ".\\scripts\\Test SMB -t existsShareName$",
    "test:name:delSharedDirectory": ".\\scripts\\Test SMB -t delSharedDirectory$",
    "test:name:connect": ".\\scripts\\Test SMB -t ^^connect$",
    "test:name:connectSync": ".\\scripts\\Test SMB -t ^^connectSync$",
    "test:name:showCurrentSession": ".\\scripts\\Test SMB -t showCurrentSession$",
    "test:name:getActiveConnectionsSwbemObjs": ".\\scripts\\Test SMB -t getActiveConnectionsSwbemObjs$",
    "test:name:getActiveConnectionsObjs": ".\\scripts\\Test SMB -t getActiveConnectionsObjs$",
    "test:name:hasConnection": ".\\scripts\\Test SMB -t hasConnection$",
    "test:name:disconnect": ".\\scripts\\Test SMB -t disconnect$",
    "test:name:disconnectSync": ".\\scripts\\Test SMB -t disconnectSync$",
    "test:name:connectSyncSurely": ".\\scripts\\Test SMB -t connectSyncSurely$",
    "test:SMB": ".\\scripts\\Test SMB",
    "build": "wsh-packager bundle ./Package.wsf",
    "test:dist:module": ".\\scripts\\Test dist:module",
    "test:dist:bundle": ".\\scripts\\Test dist:bundle",
    "test": "run-s test:dist:*",
    "predocs": "rimraf ./docs",
    "docs:common": "jsdoc --configure ./jsdoc.json",
    "cp-docs-css": "cpx ./assets/styles/jsdoc.css ./docs/styles/",
    "cp-docs-img": "cpx ./assets/img/**/* ./docs/img/",
    "postdocs": "run-p cp-docs-*",
    "docs": "run-s docs:*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo @FIXME \"npx lint-staged\" fixes all .js!"
    }
  },
  "lint-staged": {
    "*.{js}": "eslint --fix",
    "*.{json,css,md,yml,yaml}": "prettier --write"
  },
  "dependencies": {},
  "devDependencies": {
    "@tuckn/insert-gtag": "^1.1.0",
    "@tuckn/wsh-packager": "^1.0.0",
    "@types/activex-scripting": "^1.0.7",
    "@types/windows-script-host": "^5.8.3",
    "cpx": "^1.5.0",
    "docdash": "^1.2.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-jsdoc": "^24.0.2",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.2.5",
    "jsdoc": "^3.6.4",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.5",
    "rimraf": "^3.0.2"
  }
}
